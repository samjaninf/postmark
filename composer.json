{
    "name": "dirtsimple/postmark",
    "description": "Sync WP content from Markdown files",
    "type": "wp-cli-package",
    "homepage": "https://github.com/dirtsimple/postmark/",
    "license": "MIT",
    "authors": [
        {
            "name": "PJ Eby",
            "homepage": "https://github.com/pjeby"
        }
    ],
    "keywords": [
        "markdown",
        "wordpress",
        "wp",
        "wp-cli",
        "yaml",
        "front-matter",
        "content-management",
        "configuration-management",
        "static-site-generator",
        "yaml front matter"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "files": [ "command.php" ],
        "psr-4": {
            "dirtsimple\\Postmark\\": "src/",
            "Webuni\\CommonMark\\AttributesExtension\\": "ext/attrs/"
        }
    },
    "require": {
        "wp-cli/wp-cli": "^2.0.0|dev-master",
        "rarst/wpdatetime": "^0.3.0",
        "league/commonmark": "^0.17.5",
        "league/commonmark-extras": "^0.1.4",
        "webuni/commonmark-table-extension": "^0.8.0",
        "onemorething/commonmark-strikethrough-extension": "^0.13.0",
        "twig/twig": "^2.4"
    }
}
